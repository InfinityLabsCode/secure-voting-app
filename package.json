{
  "name": "secure-voting-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start:local": "env-cmd -f environments/.env.local next dev",
    "start:development": "env-cmd -f environments/.env.development next dev",
    "start:rc": "env-cmd -f environments/.env.rc next dev",
    "start:production": "env-cmd -f environments/.env.production next dev",
    "build:development": "env-cmd -f environments/.env.development next build && next export -o build",
    "build:local": "env-cmd -f environments/.env.local next build && next export -o build",
    "build:rc": "env-cmd -f environments/.env.rc next build && next export -o build",
    "build:production": "env-cmd -f environments/.env.production next build && next export -o build",
    "cypress:open": "cypress open",
    "test": "start-server-and-test dev 3000 cypress:open",
    "prettier-format": "prettier --write './**/*.{ts,tsx}'",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "applypatch-msg": "echo \"[Husky] applypatch-msg\"",
      "pre-applypatch": "echo \"[Husky] pre-applypatch\"",
      "post-applypatch": "echo \"[Husky] post-applypatch\"",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx,ts,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "env-cmd": "^10.1.0",
    "ethers": "^5.7.1",
    "lodash": "^4.17.21",
    "next": "12.2.5",
    "postcss": "^8.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spinners": "^0.13.4",
    "react-toastify": "^9.0.8",
    "sass": "^1.54.4",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.184",
    "@types/node": "18.7.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.2"
  }
}
